{
  "name": "smart-exhibitor-portal",
  "version": "1.0.0",
  "description": "Self-service onboarding portal for trade show exhibitors",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "express-session": "^1.17.3",
    "connect-mongo": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["exhibitor", "portal", "trade-show", "onboarding"],
  "author": "Smart Exhibitor Portal Team",
  "license": "MIT"
} 